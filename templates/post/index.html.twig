{% extends 'base.html.twig' %}

{% block title %}Hello PostController!{% endblock %}

{% block body %}
<div class="posts">
    {% for post in posts %}
    <div class="post">
        <div class="title">{{ post.title }}</div>
        <div class="text">{{ post.text }}</div>
    </div>
    {% endfor %}
</div>
{% endblock %}
